{
  "version": 1,
  "projects": {
    "webcomponents": {
      "projectType": "library",
      "schematics": {
        "@nxext/stencil:component": {
          "style": "scss",
          "storybook": false
        }
      },
      "root": "libs/webcomponents",
      "sourceRoot": "libs/webcomponents/src",
      "architect": {
        "build": {
          "builder": "@nxext/stencil:build",
          "options": {
            "projectType": "library",
            "configPath": "libs/webcomponents/stencil.config.ts"
          }
        },
        "test": {
          "builder": "@nxext/stencil:test",
          "options": {
            "projectType": "library",
            "configPath": "libs/webcomponents/stencil.config.ts"
          }
        },
        "e2e": {
          "builder": "@nxext/stencil:e2e",
          "options": {
            "projectType": "library",
            "configPath": "libs/webcomponents/stencil.config.ts"
          }
        },
        "serve": {
          "builder": "@nxext/stencil:build",
          "options": {
            "projectType": "library",
            "configPath": "libs/webcomponents/stencil.config.ts",
            "serve": true,
            "watch": true
          }
        }
      }
    }},
  "cli": {
    "defaultCollection": "@nxext/stencil"
  }
}
